generator client {
  provider = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider = "mysql"
}

model Lead {
  id        String   @id @default(cuid())
  name      String
  email     String
  company   String?
  message   String
  source    String?
  createdAt DateTime @default(now())

  @@index([email])
  @@index([createdAt])
}
